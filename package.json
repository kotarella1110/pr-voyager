{
  "name": "pr-voyager",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && ncc build src/index.ts --source-map --minify",
    "check": "run-s check:*",
    "check:tsc": "tsc --noEmit",
    "check:prettier": "prettier . --ignore-path .gitignore --ignore-path .prettierignore --check",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier . --ignore-path .gitignore --ignore-path .prettierignore --write --cache",
    "changeset": "changeset",
    "bump": "node ./scripts/bump.js",
    "release": "node ./scripts/release.js"
  },
  "author": "kotarella1110",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@manypkg/get-packages": "^2.2.0",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^24.10.4",
    "@vercel/ncc": "^0.36.1",
    "changesets": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.4"
  }
}
